{
  "name": "sendhut",
  "main": "app.js",
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "gulp watch",
    "build": "gulp build",
    "deploy": "gulp deploy",
    "test": "ava --tap | tap-nyan",
    "heroku-postbuild": "npm run build-emails",
    "build-emails": "mkdir -p ../templates/templated_email/compiled/ && find ../templates -name \"*.mjml\" -not -path \"../templates/templated_email/source/*partials/*\" -not -path \"../templates/templated_email/source/*shared/*\" -exec ./node_modules/.bin/mjml -l skip {} -o ../templates/templated_email/compiled/ \\;"
  },
  "author": "Maurizio Mangione",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.0.0-beta",
    "form-serializer": "^2.5.0",
    "gulp-cache": "^1.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-minify": "^2.1.0",
    "gulp-size": "^2.1.0",
    "jquery": "^3.2.1",
    "js-cookie": "^2.2.0",
    "mjml": "^4.0.3",
    "popper.js": "1.12.6",
    "sass-rem": "^2.0.0",
    "scrollpos-styler": "^0.7.0",
    "source-sans-pro": "git://github.com/adobe-fonts/source-sans-pro.git#release"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.32",
    "@babel/preset-env": "^7.0.0-beta.32",
    "autoprefixer": "^7.1.6",
    "ava": "^0.22.0",
    "babel": "^6.5.2",
    "babel-core": "^6.7.7",
    "babel-eslint": "^8.0.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.3.0",
    "browser-sync": "^2.12.5",
    "browserify": "^14.5.0",
    "cssnano": "^3.5.2",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-eslint": "^4.0.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-html-replace": "^1.5.5",
    "gulp-imagemin": "^3.0.1",
    "gulp-notify": "^3.0.0",
