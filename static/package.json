    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-uglify": "^3.0.0",
    "holderjs": "^2.9.4",
    "imagemin-pngquant": "^5.0.0",
    "lodash": "^4.11.1",
    "postcss-nested": "^2.1.2",
    "postcss-simple-extend": "^1.0.0",
    "postcss-simple-vars": "^4.1.0",
    "react-addons-test-utils": "^15.0.1",
